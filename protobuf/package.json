{
  "name": "protobuf",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile-ts": "protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts_proto --experimental_allow_proto3_optional --ts_proto_out=../generated/typescript  --ts_proto_opt=outputServices=grpc-js,esModuleInterop=true --proto_path=. game/*.proto global/*.proto player/*.proto",
    "compile-py": "protoc --python_out=../generated/python --proto_path=. game/*.proto global/*.proto player/*.proto",
    "compile-go": "protoc --go_out=../generated/go --proto_path=. game/*.proto global/*.proto player/*.proto",
    "compile-docs": "protoc --doc_out=../generated/docs --doc_opt=markdown,docs.md --proto_path=. game/*.proto global/*.proto player/*.proto"
  },
  "author": "Brian <brian@oof.gg> (https://www.oof.gg/)",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "ts-proto": "^2.2.5",
    "@grpc/grpc-js": "^1.12.2",
    "@grpc/proto-loader": "^0.7.13"
  }
}
