{
  "compilerOptions": {
    "paths": {
      "@protos/*": ["../../generated/typescript/*"],
      "@oof.gg/protobuf-ts/*": ["node_modules/@oof.gg/protobuf-ts/*"],
    },
    "target": "es2017",                                  
    "module": "commonjs",                                
    "esModuleInterop": true, 
    "forceConsistentCasingInFileNames": true,
    "strict": true,   
    "skipLibCheck": true,
    "baseUrl": "./",
    "outDir": "dist",
  },
}

